<launch>
  <node pkg="joy" type="joy_node" name="joystick"/> 

<!--   <node pkg="jc_base" type="jc_base_node" name="jc_base_node" output="screen" /> -->

  <node pkg="mission_control" type="mission_control_node" name="mission_control_node" output="screen"  cwd="node"/>

  <node pkg="obstacle_manager" type="obstacle_manager_node" name="obstacle_manager_node" />

  <!-- <node pkg="sonic_manager" type="sonic_manager_node" name="sonic_manager_node" /> -->

  <node pkg="map_server" type="map_server" name="map_server_node" args="$(find config)/map/1.yaml" output="screen">
    <param name="map_folder"  value="$(find config)/map/"/>
  </node>

  <node pkg="rviz" type="rviz" name="rviz_navi" output="screen" args="-d $(find config)/cfg/rviz_config.rviz" />

</launch>
