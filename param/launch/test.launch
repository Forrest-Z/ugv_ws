<launch>
  <node pkg="joy" type="joy_node" name="joystick" respawn="true">
      <param name="dev" value="/dev/input/js0" />
  </node>
  
  <node name="speed_manager" pkg="mission_control" type="speed_manager" output="screen" respawn="true">      
      <param name="max_rotation"  value="0.6"/>     
      <param name="max_speed"  value="0.8"/>
      <param name="vehicle_radius"  value="0.4"/>
      <param name="lookahead_time"  value="2"/>   
      <param name="log_gap_sec"  value="0"/> 
      <param name="safe_zone"  value="5"/> 
      <param name="force_constant_x"  value="0.5"/> 
      <param name="force_constant_y"  value="0.5"/> 
  </node>



 </launch>