<launch>
  
  <node name="speed_manager" pkg="mission_control" type="speed_manager" output="screen" respawn="true">
    <rosparam>
      max_rotation: 0.6
      max_speed: 0.8
      vehicle_radius: 0.6
      lookahead_time: 4
      safe_zone: 5
      force_constant_x: 0.4
      force_constant_y: 0.02

      max_height: 2
      min_height: 0.5
      max_range: 8
      min_range: 0.2
      oscillation: 0.1

      one_hand: false

      goal_a: [397.267,-14.144]
      goal_b: [250.257,49.591]
      goal_x: [-125.037,-1.265]
      goal_y: [-47.731689,43.224480]
    </rosparam>  
    
  </node>

  <node name="navi_manager" pkg="mission_control" type="navi_manager" output="screen" respawn="true">    
    <rosparam>
      use_sim: true
      speed_scale: 0.3
      rotation_scale: 0.5
      lookahead_distance: 3
    </rosparam>
      <param name="junction_file"  value="$(find param)/map/junction.txt"/>
  </node>

  
</launch>